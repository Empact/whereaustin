<div id="listings">
<% for type in @types -%>
<h1 id="<%= type.id -%>" class="section"><%= type.name -%></h1>
<ul class="eventList <%= type.id -%>">
  <% for event in @events[type.id] -%>
  <li><a title="<%= event.venue.address || "" -%>">
    <span class="venue"><%= event.venue.name %></span><br />
    <span class="act"><%= event.name %></span></a>
  </li>
  <% end -%>
</ul>
<% end -%>